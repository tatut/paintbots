  },
  "dependencies": {
    "axios": "^1.3.5"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "18.15.11",
    "@types/prettier": "2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.0.3",
    "prettier": "2.8.7",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
